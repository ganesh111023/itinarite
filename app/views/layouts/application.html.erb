    <!--header end-->

    <!--sidebar start-->
    <aside>
    <div id="sidebar"  class="nav-collapse ">
      <!-- sidebar menu start-->
      <ul class="sidebar-menu" id="nav-accordion">
          <li>
              <a href="#;">
                  <i class="fa fa-dashboard"></i>
                  <span>New feeds</span>
              </a>
          </li>
          <li>
              <a href="#;">
                  <i class="fa fa-envelope-o"></i>
                  <span>Message</span>
              </a>
          </li>
          <li>
              <a href="#;">
                  <i class="fa fa-code-fork"></i>
                  <span>Followers</span>
              </a>
          </li>
          <li>
              <a href="#;">
                  <i class="fa fa-calendar-o"></i>
                  <span>Events</span>
              </a>
          </li>
          <li>
              <a href="#">
              <i class="fa fa-car" aria-hidden="true"></i>
                  <span>My Trips</span>
              </a>
          </li>
          <!--multi level menu end-->
      </ul>
      <!-- sidebar menu end-->
    </div>
    </aside>
    <!--sidebar end-->

    <%= yield  %>

    
    <!--footer start-->
    <footer class="site-footer">
    <div class="text-center">
      2016 &copy; itinarite.
      <a href="#" class="go-top">
          <i class="fa fa-angle-up"></i>
      </a>
    </div>
    </footer>
    <!--footer end-->
    </section>
    <!--footer end-->
    
  </body>
