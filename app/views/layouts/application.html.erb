  <link href="/assets/font-awesome.css" rel="stylesheet" />

  <link rel="stylesheet" type="text/css" href="/assets/jquery-ui-1.10.1.custom.min.css">
  <link rel="stylesheet" type="text/css" href="/assets/bootstrap-datetimepicker.min.css">

  <!--right slidebar-->
  <link href="/assets/slidebars.css" rel="stylesheet">
  <link href="/assets/style-responsive.css" rel="stylesheet" />
  <link rel="stylesheet" href="/assets/swiper.min.css">
  <link rel="stylesheet" type="text/css" href="/assets/fileinput.min.css">
